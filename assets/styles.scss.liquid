/* #Home page - featured promotions
================================================== */
.featured-promotions-section {
  .promo-animation--true {
    opacity: 0;
  }
  
  .featured-promotion-group {
    display: flex;
    flex-direction: row;
    margin-bottom: 5px;
    
    @include respond-to('small') {
      display: block;
      margin-bottom: 0;
    }
    
    .featured-item {
      flex:1;
      margin-right: 5px;
      
      &.one-quarter {
        max-width: 25%;
      }
      &.one-third {
        max-width: 33.3333333333333%;
      }
      &.one-half {
        max-width: 50%;  
      }
      &.two-thirds {
        max-width: 66.666666666666%;
      }
      &.three-quarters {
        max-width: 75%;
      }
      &.full {
        max-width: 100%;
      }
      &:last-child {
        margin-right: 0;
      }
      
      @include respond-to('small') {
        max-width: 100% !important;
        width: 100%;
        display: block;
        margin-right: 0;
        margin-bottom: 5px;
      }
      
      figure {
        position: relative;
        display: flex;
        height: auto;
        min-width: 100%;
        min-height: 100%;
        @include respond-to('small') {
          display: block;
          min-height: initial;
          min-width: initial;
        }
        
        img, svg {
          display: block;
          width: 100%;
          height: auto;
          object-fit: fill;
          align-self: center;
          
          &.mobile-image {
            display: none;
          }
          
          @include respond-to('small') {
            display: none;
            &.mobile-image {
              display: block;
            }
          }
        }
        
        figcaption, a {
          position: absolute;
          top: 0;
          display: flex;
          flex-direction: column;
          min-height: 100%;
          min-width: 100%;
          align-items: center;
          justify-content: center;
          h2, p, button {
            font-size: 40px;
            font-weight: bold;
            font-stretch: expanded;
            color:white;
          }
          h2 {
            font-size: 40px;
            text-shadow: 0 0 5px rgba(0,0,0,0.2);
            margin: 1rem;
          }
          .details {
            text-align: center;
          }
          p {
            font-size: {{ settings.regular_font_size }}px;
            height: 0;
            text-shadow: 0 0 5px rgba(0,0,0,0.2);

          }
          .button {
            border-color: white;
            border-width: 2px;
            font-size: 30px;
            font-weight: bold;
            padding: 28px 81px;
           	background: transparent;
            color: white;
            text-shadow: 0 0 2px rgba(0,0,0,0.2);

          }
        }
      }
    }
  }
}

$i: 1;
$delay: 0;
@while $i < 13 {
  .promo-#{$i} {
    -webkit-animation-delay: $delay + s;
    animation-delay: $delay + s;
  }
  $i: $i + 1;
  $delay: $delay + 0.3;
}

