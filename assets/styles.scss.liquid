      line-height: 1em;
      padding: 15px 0;
      min-height: auto;
      width: 100%;
      .icon-down-arrow {
        display: block;
      }
    }
  }
  
  // disable accordions on larger views
  .toggle_content {
    display: block;
    max-height: auto;
    overflow: hidden;
    
    @include respond-to('small') {
      transition: max-height .3s ease;
      max-height: 0;
    }
  }
  
  h3.active + .toggle_content {
    
    @include respond-to('small') {
      display: block;
      max-height: 450px;
    }
  }
  
  ul {
    list-style-type: none;
    padding: 0;
    font-size: 12px;
    line-height: 12px;
    
    li {
      margin: 12px 0;
      &:first-child {
        margin-top: 0;
      }
    }
  }
  
  .footer-block {
    margin: 0 42px 32px;
    &:first-of-kind {
      margin-left: 0;
    }
    &:last-of-kind {
      margin-right: 0;
    }
    padding: 0 8px;

    /* grid mode
    &.sixth { grid-column: auto / span 10; }
    &.fifth { grid-column: auto / span 12; }
    &.quarter { grid-column: auto / span 15; }
    &.third { grid-column: auto / span 20; }
    &.two-fifths { grid-column: auto / span 24; }
    &.half { grid-column: auto / span 30; }
    &.two-thirds { grid-column: auto / span 40; }
    &.three-quarters { grid-column: auto / span 45; }
    &.four-fifths { grid-column: auto / span 48; }
    &.five-sixths { grid-column: auto / span 50; }
    &.full { grid-column: auto / span 60; }
    */
    /* flex mode */
    &.sixth { max-width: 200px; }
    &.fifth { max-width: 240px; }
    &.quarter { max-width: 300px; }
    &.third { max-width: 400px; }
    &.two-fifths {  max-width: 480px; }
    &.half { max-width: 600px; }
    &.two-thirds { max-width: 800px; }
    &.three-quarters { max-width: 900px; }
    &.four-fifths { max-width: 960px; }
    &.five-sixths { max-width: 1000px; }
    &.full { max-width: 1200px; }

    @include respond-to('medium') {
      width: auto;
      margin: 0 auto;
      &.sixth, &.fifth, &.quarter, &.third, { flex-basis: 33.3333%; width: 400px; max-width: initial; }
      &.two-fifths, &.half, &.two-thirds, &.three-quarters, &.four-fifths, &.five-sixths, &.full { flex-basis: 100%; width: 1200px; }
    }

    @include respond-to('small') {
      border-bottom: 1px solid #999;
      &:last-of-type {
        border-bottom: none;
      }
      width: auto;
      margin: 0 auto;
      &.sixth, &.fifth, &.quarter, &.third, { width: auto; max-width: initial; }
      &.two-fifths, &.half, &.two-thirds, &.three-quarters, &.four-fifths, &.five-sixths, &.full { width: auto; max-width: initial; }
    }
    
    > div {
      display: flex;
      flex-direction: column;
      align-items: flex-start;
      
      h3 {
        flex-shrink: 0;
        flex-grow: 0;
      }
    }
  }
  
  .credits {
    grid-column: auto / span 60;
    margin: 40px 0;
  }
  
  .newsletter_section {
    position: relative;
    max-width: 480px;
    margin: 0 auto;
    
