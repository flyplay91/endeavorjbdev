/*
 * #Collections
 * master styles for collections page
  ==================================================== */
.collection {
  
  .sidebar {
    top: 210px;
    position: fixed;
    bottom: 0%;
    z-index: 15;
    margin-right: 0;
    
    @include respond-to('medium') {
      display: none;
    }
    
    .sidebar-wrap {
      padding-left: 20px;
      padding-right: 10px;
      background: white;
    }
    
    .sidebar-block {
      /* use native font for the +/- icons used in menus */
      .icon-down-arrow:before, .icon-up-arrow:before {
        font-family: {{ settings.regular_font | replace: 'Google_', '' | replace: '+', ' ' }};
        font-style: normal;
        font-weight: 100;
        font-size: 24px;
        margin-top: -8px;
        margin-right: 0;
        content: "\2335";
      }
      .icon-up-arrow:before { transform: rotateX(180deg) translateY(-50%); }
    }
  } /* .sidebar */
    
  .load-more__icon {
    display: block;
    height: 32px;
    width: 32px;
    transition: all 0.3s linear;
    background: url('{{ 'loader.gif' | asset_url | split:'?' | first }}') center center no-repeat;
    background-size: 32px 32px;
    margin: 0 auto;
  }
  
  .product-grid-content {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    grid-column-gap: 8px;
    grid-row-gap: 15px;
    width: 100%;
    max-width: calc(1600px - 195px - 8px);
    padding: 0 8px 0 195px;
    margin: 0 auto;
    
    @include respond-to('medium') {
      grid-template-columns: repeat(2, 1fr);
      padding: 0 4px;
    }

    
    .product-grid-item {
      display: block;
      position: relative;
      text-align: center;
      
      .swiper-container {
        opacity: 0;
        position: absolute;
        top: 0;
        left: 0;
        z-index: 2;
        width: 100%;
        height: auto;
        .swiper-wrapper * {
          min-width: 100%;
        }
        .swiper-button-prev {
          left: 0;
        }
        .swiper-button-next {
          right: 0;
        }
        @include respond-to("medium") {
          display: none;
        }
      }
      
      @media (any-hover: hover) {
        &:hover .hover-size-optin {
          opacity: 1;
          transform: none;
        }
        &:hover .img-arrow {
          opacity: 1;
        }
        &:hover {
          .prod-image-wrap {
            opacity: 0;
            visibility: hidden;
            @include respond-to('medium') {
              opacity:1;
              visibility:visible;
            }
          }
          .swiper-container {
            opacity: 1;
          }
        }
      }
      .thumbnail-overlay {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        opacity: 0;
        overflow: hidden;
        transition: opacity 0.3s ease-in;
        > a {
          display: block;
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
        }
        @include respond-to('medium') {
          display: none;
        }
        @include respond-to('xlarge') {
          line-height: 1.5;
        }
      }
      .image__container {
        padding-bottom: 150%;
        position: relative;
        .prod-image-wrap {
          position: absolute;
          width: 100%;
          top: 0;
          left: 0;
        }
      }
      .hover-size-optin {
        position: absolute;
        bottom: 0;
        margin-bottom: -1px; /* this more-or-less fixes 1px gaps caused by fractional pixel heights */
        z-index: 3;
        width: calc(100%);
        opacity:0;
        background-color: rgba(255, 255, 255, 0.8);
        .sizelist {
          display: flex;
          justify-content: center;
          padding: 0px;
          form {
            display: inline-block;
            margin: 0 3px;
            button {
              color: #000;
              background-color: transparent;
              border-radius: 0;
              width: auto;
              padding: 0.5em 1.5em;
              height: auto;
              min-height: 2.5em;
              font-family: {{ settings.headline_font | replace: 'Google_', '' | replace: '+', ' ' }};
              line-height: 1.5em;
              font-size: 12px;
              margin: 0px;
              border: 0 none;
              &:hover {
                color: white;
                background: black;
              }
            }
          }
          @include respond-to('small') {
            display:none;
          }
        }
      }
      .product-details {
        margin-top: 8px;
        font-size: 12px;
        .title {
          color: #4a4a4a;
        }
        .title:hover {
          color: #000;
        }
        .price{
          .money {
            color: {{ settings.regular_color }};
          }
          > .was_price {
            color: {{ settings.was_price_color }};
            margin-left: 4px;
          }
        }
        .color-count {
          display: none;
        }
      }
    }
  }
}


