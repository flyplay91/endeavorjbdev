/* #Swatch Styles
================================================== */

/* SASS variables for swatch dimensions */
$swatch-element-size: 35px;
$swatch-label-size: 34px;
$soldoutcolor: #bfbfbf;

.swatch-element.swatch--active {
  border-color: black;
  box-shadow: 0px 0px 0px 2px rgba(255,255,255,1);
}

.selector-wrapper label {margin-bottom: 0.5em;}

.product_section .product_form {
  opacity: 1;
}

.multi_select,
form .select {
  display: block;
}

{% if settings.product_form_style == "swatches" %}

.product_section .product_form {
  opacity: 1;
  transition: opacity 0.2s ease;
}

.product_section .product_form.is-visible {
  max-width: 400px;
  opacity: 1;
}

.selector-wrapper, .select { display: none; }

/** label styles apply to both the swatches and the notify me when sold out labels */
.swatch .swatch-element, .ssw {
  label {
    float:left;
    min-width: $swatch-element-size;
    height: $swatch-element-size;
    margin:0;
    padding: 0 8px;
    text-align:center;
    line-height: $swatch-element-size - 3px;
    white-space:nowrap;
    cursor: pointer;
    font-size: 12px !important;
    font-weight: 500;
    font-style: normal;
    font-stretch: normal;
    letter-spacing: normal;
    color: black;
    border: 1px solid black;
  }
  
  &.act label {
    background-color: black;
    color: white;
  }

  &.soldout label {
    filter: alpha(opacity=60);
    -khtml-opacity: 0.6;
    -moz-opacity: 0.6;
    opacity: 0.6;
    color: $soldoutcolor;
    border-color: $soldoutcolor;
  }

  &.color label {
    padding:0;
    margin: 0;
    width: $swatch-label-size;
    height: $swatch-label-size;
  }
} /* /.swatch-element /.ssw */


input.product_size:checked + div.swatch-element label {
  background: black;
  color:white;
  border-color: black;
}


.crossed-out {
  @include diagonal-line;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}

.swatch {
  .option_title {
    margin: 0.5em 0;
  }
  
  input {
    display: none;
  }
  
  .color {
    display: none;
  }

  .color label {
    min-width: $swatch-label-size;
    height: $swatch-label-size !important;
    line-height: $swatch-label-size;
    background-position: center;
    background-size: cover;
  }

  .swatch-element {
    min-width: $swatch-element-size;
    min-height: $swatch-element-size;

    &.color {
      display: none;
      padding: 3px;
    }


    &.soldout .crossed-out {
      {% if settings.swatch_style != 'square' %}
        border-radius: 50%;
      {% endif %}
      display:block;
      height: 100%;
      width: 100%;
      pointer-events: none;
    }

  } /* /.swatch-element */
} /* /.swatch */



/* Tooltips */
.swatch .tooltip {
  text-align:center;
  background: #333;
  background: rgba(#333, 0.9);
  color:#fff;
  bottom:100%;
  padding: 5px 10px;
  display:block;
  position:absolute;
  width:120px;
  font-size: 13px;
  left:{{ 30 | divided_by: 2 | minus: 50 }}px;
  margin-bottom:15px;
  /* Make it invisible by default */
  filter:alpha(opacity=0);
  -khtml-opacity: 0;
  -moz-opacity: 0;
  opacity:0;
  visibility:hidden;
  /* Animations */
  -webkit-transform: translateY(10px);
  -moz-transform: translateY(10px);
  -ms-transform: translateY(10px);
  -o-transform: translateY(10px);
  transform: translateY(10px);
  transition: all .25s ease-out;
  box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.28);
  z-index: 10000;
  
  &:before {
    bottom:-20px;
    content:" ";
    display:block;
    height:20px;
    left:0;
    position:absolute;
    width:100%;
  }
  
  &:after {
    /* CSS triangle */
    border-left:solid transparent 10px;
    border-right:solid transparent 10px;
    border-top:solid rgba(#333, 0.9) 10px;
    bottom:-10px;
    content:" ";
    height:0;
    left:50%;
    margin-left:-13px;
    position:absolute;
    width:0;
  }
}

.swatch .swatch-element:hover .tooltip {
  filter:alpha(opacity=100);
  -khtml-opacity:1;
  -moz-opacity:1;
  opacity:1;
  visibility:visible;
  -webkit-transform:translateY(0px);
  -moz-transform:translateY(0px);
  -ms-transform:translateY(0px);
  -o-transform:translateY(0px);
  transform:translateY(0px);
}
{% endif %}

