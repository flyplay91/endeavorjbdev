          &::after {
            line-height:initial;
            position:initial;
            bottom:auto;
            right:auto;
          }
        }
      }
      input.quantity {
        padding:0;
        font-size: 12px;
        background: transparent;
      }
      .product-plus .icon-plus::after,
      .product-minus .icon-minus::after {

      }
    }
  }

  .cart_content {
