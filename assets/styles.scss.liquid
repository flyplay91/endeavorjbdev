    /* note overlay & mini-cart animations and transitions are handled in mini-cart.js */
    .cart-overlay {
      z-index:80;
      position: fixed;
      background-color: white;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
    }

    .cart_content {
      transform: translateX(100%);
      transition: transform 0.5s ease;
    }
    
    .cart-logo {
      width: 100px;
      height: 21px;
      background: url("{{ "joah-brown-logo.png" | asset_url }}") no-repeat;
      background-size: contain;
    }
    
    .cart_content.open {
      transform: translateX(0%);
    }

    /** general layout below **/
    .cart-header {
      text-align: center;
      .cart-logo {
        width: 50%;
        margin: 0 auto 30px;
        @include respond-to('large'){
          margin: 0 auto 20px;
        }
        @include respond-to('xlarge'){
          margin: 0 auto 30px;
        }
        @include respond-to('medium'){
          margin: 0 auto 10px;
        }
      }
      .cart-close {
        float: left;
        margin-left: 20px;
        .icon-cross:before {
          font-size: 12px;
        }
      }
    }

    .cart_container.active_link .cart_content::-webkit-scrollbar {
      -webkit-appearance: none;
      width: 7px;
    }
    .cart_container.active_link .cart_content::-webkit-scrollbar-thumb {
      border-radius: 0;
      background-color: rgba(0,0,0,.5);
      -webkit-box-shadow: 0 0 1px rgba(255,255,255,.5);
    }

    .cart_container {
      max-height: 100vh;
      position: relative;
      hr {
        border-top-width: 0px;
      }
      a {
        margin-right: 20px;
        margin-top: 30px;
        &.cart-close { margin-top: 0; }
      }
    }


    /**** Force relayout of quantity box to a flex box ****/
    .cart_content .product-quantity-box,
    #cart_form .product-quantity-box {
      display: flex;
      justify-content: space-between;
      align-items: stretch;
      max-width: 3rem;
      position: absolute;
      right: 0;
      bottom: 10px;
      > span.product-minus, span.product-plus {
        flex-shrink:1;
        flex-grow:0;
