/* #Basic Styles
================================================== */
html {
  padding-bottom: 0 !important;
  /*background-color:#000;*/
}

body {
  font-family: {{ settings.regular_font | replace: 'Google_', '' | replace: '+', ' ' }} !important;
  font-weight: {{ settings.regular_weight }};
  font-size: {{ settings.regular_font_size }}px;
  text-transform: {{ settings.regular_font_style }};
  color: {{ settings.regular_color }};
  line-height: 1em;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
