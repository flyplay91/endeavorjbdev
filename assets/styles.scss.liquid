.footer {
  /* grid mode
  display: grid;
  max-width: 1200px;
  margin: 72px auto 0;
  padding: 0 8px;
  grid-template-columns: repeat(60, 1fr);
  grid-template-rows: auto;
  grid-row-gap: 32px;
  */
  /* flex mode */
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  max-width: 1200px;
  flex-wrap: wrap;
  
  a, a:visited {
    color: #4a4a4a;
  }
  
  .flex-container {
    display: inline-flex;
    width: auto;
    margin: 72px auto 0;
    padding: 0 8px;
    flex-wrap: wrap;
    justify-content: center;
  }
  
  > .credits {
    flex-basis: 100%;
  }
  /* end flex stuff */
  
  @include respond-to('small') {
    display: block;
    
    .flex-container {
      display: block;
    }
  }
  
  h3 {
    font-size: 15px;
    line-height: 1.5em;
    padding: 0;
    margin-bottom: 5px;
      
    .icon-down-arrow {
      display: none;
    }
    
    @include respond-to('small') {
