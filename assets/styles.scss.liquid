/* Animation for checkmark on add to cart button */

button.add_to_cart {
  position: relative;
  width: 100%;

  .text {
    display: block;
    width: 100%;
    -webkit-animation-duration: 0.5s;
    animation-duration: 0.5s;
  }
  .fadeInDown.text {
    -webkit-animation-duration: 0.8s;
    animation-duration: 0.8s;
  }
}

button
.checkmark {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  path {
    stroke-dasharray: 19.79 19.79;
    stroke-dashoffset: 19.79;
    opacity: 0;
  }
  &.checkmark-active path {
    -webkit-animation: drawCheckmark 0.25s linear alternate forwards;
    animation: drawCheckmark 0.25s linear alternate forwards;
  }
}

@keyframes drawCheckmark {
  from {
    stroke-dashoffset: 19.79;
    opacity: 1;
  }
  to {
    stroke-dashoffset: 0;
    opacity: 1;
  }
}

@-webkit-keyframes drawCheckmark {
  from {
    stroke-dashoffset: 19.79;
    opacity: 1;
  }
  to {
    stroke-dashoffset: 0;
    opacity: 1;
  }
}

.ie
button
.checkmark {
  path {
    stroke-dashoffset: 0;
    opacity: 0;
  }
  &.checkmark-active path{
    -webkit-animation: fadeCheckmark 0.5s linear alternate forwards;
    animation: fadeCheckmark 0.5s linear alternate forwards;
  }
}

@-webkit-keyframes fadeCheckmark {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

@keyframes fadeCheckmark {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

@include respond-to('ie10+'){
  button
  .checkmark {
    path {
      stroke-dashoffset: 0;
      opacity: 0;
    }
    &.checkmark-active path{
      animation: fadeCheckmark 0.5s linear alternate forwards;
    }
  }

  @keyframes fadeCheckmark {
    from {
      opacity: 0;
    }
    to {
      opacity: 1;
    }
  }
}

